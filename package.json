  "dependencies": {
    "@azure/msal-node": "^3.5.3",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/session": "^11.1.0",
    "@fastify/static": "^8.1.1",
    "dotenv": "^16.5.0",
    "fastify": "^5.2.2",
