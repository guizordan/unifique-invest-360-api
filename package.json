{
  "name": "unifique-api",
  "version": "1.0.0",
  "description": "API for Unifique using Fastify, Sequelize, and TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.ts",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "build": "tsc",
    "create-admin": "ts-node src/create-admin.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "db:migrate": "sequelize-cli db:migrate --config ./src/infrastructure/database/config/sequelize.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guizordan/unifique-api.git"
  },
  "keywords": [],
  "author": "guilhermezordan@outlook.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guizordan/unifique-api/issues"
  },
  "homepage": "https://github.com/guizordan/unifique-api#readme",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/passport": "^3.0.2",
    "@fastify/session": "^11.1.0",
    "@fastify/static": "^8.1.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.2",
    "jsonwebtoken": "^9.0.2",
    "mailersend": "^2.5.0",
    "mysql2": "^3.14.0",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "puppeteer": "^24.6.0",
    "sequelize": "^6.37.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.0",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "prompt-sync": "^4.2.0",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "type": "module"
}
